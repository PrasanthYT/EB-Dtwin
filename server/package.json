{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "update-config": "npm install git+https://$GITHUB_TOKEN@github.com/Di-Twin/Back-End-Config.git#prod",
    "update-ml": "npm install git+https://$GITHUB_TOKEN@github.com/Di-Twin/ML-Score_function.git",
    "update-db": "npm install git+https://$GITHUB_TOKEN@github.com/Di-Twin/Database.git",
    "update-all": "npm run update-config && npm run update-ml && npm run update-db",
    "sync-fitbit-sleep": "node -e \"require('./workers/fitbitSyncWorker').syncSleepData()\"",
    "sync-fitbit-activity": "node -e \"require('./workers/fitbitSyncWorker').syncActivityData()\"",
    "sync-fitbit-all": "node -e \"const worker = require('./workers/fitbitSyncWorker'); Promise.all([worker.syncSleepData(), worker.syncActivityData()]).then(() => console.log('All sync complete'))\"",
    "start-notifications": "node -e \"require('./services/notification').initNotificationService()\"",
    "test-notifications": "node scripts/test-notifications.js",
    "test-notifications-integration": "mocha tests/integration/notification.test.js --timeout 15000",
    "test-notifications-manual": "node scripts/manual-notification-test.js"
  },
